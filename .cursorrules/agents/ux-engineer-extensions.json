{
  "agent": "ux-engineer",
  "extensions": {
    "required": [
      "bradlc.vscode-tailwindcss",
      "naumovs.color-highlight",
      "pranaygp.vscode-css-peek"
    ],
    "autoActivate": true,
    "commands": [
      "tailwindCSS.showOutput",
      "color-highlight.activate"
    ],
    "mcpIntegration": {
      "shadcn-ui": ["vscode-tailwindcss"],
      "playwright": ["coverage-gutters"]
    }
  }
}

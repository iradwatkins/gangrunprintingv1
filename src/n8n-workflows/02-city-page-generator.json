      "parameters": {
        "functionCode": "const data = $input.item.json.body;\n\nreturn {\n  json: {\n    campaignId: data.campaignId,\n    city: data.city,\n    state: data.state,\n    productSpec: data.productSpec,\n    mainProductImageUrl: data.mainProductImageUrl\n  }\n};"
      },

      "name": "Generate City Hero Image",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 3,
      "position": [650, 500]
    },
    {
      "parameters": {
        "functionCode": "// Combine all generated content\nconst intro = $node[\"Generate Intro (Ollama)\"].json.response;\nconst benefits = $node[\"Generate Benefits (Ollama)\"].json.response;\nconst faqs = $node[\"Generate FAQs (Ollama)\"].json.response;\nconst cityImage = $node[\"Generate City Hero Image\"].json.predictions[0].bytesBase64Encoded;\n\nreturn {\n  json: {\n    campaignId: $node[\"Extract Data\"].json.campaignId,\n    city: $node[\"Extract Data\"].json.city,\n    state: $node[\"Extract Data\"].json.state,\n    slug: `${$node[\"Extract Data\"].json.city.toLowerCase()}-${$node[\"Extract Data\"].json.state.toLowerCase()}`,\n    title: `5000 4x6 Flyers in ${$node[\"Extract Data\"].json.city}, ${$node[\"Extract Data\"].json.state} | 3-4 Day Turnaround | $179`,\n    aiIntro: intro,\n    aiBenefits: benefits,\n    faqSchema: JSON.parse(faqs),\n    mainProductImageUrl: $node[\"Extract Data\"].json.mainProductImageUrl,\n    cityHeroImageBase64: cityImage\n  }\n};"
      },
      "name": "Combine Content",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [850, 300]
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "INSERT INTO \"CityLandingPage\" (id, \"landingPageSetId\", slug, title, \"aiIntro\", \"aiBenefits\", \"faqSchema\", \"mainProductImageUrl\", \"cityHeroImageUrl\", views, revenue, \"conversionRate\", \"createdAt\")\nVALUES ($1, $2, $3, $4, $5, $6, $7, $8, $9, 0, 0, 0, NOW())\nRETURNING *",
        "additionalFields": {
          "queryParameters": "={{ [`page-${Date.now()}`, $node[\"Combine Content\"].json.campaignId, $node[\"Combine Content\"].json.slug, $node[\"Combine Content\"].json.title, $node[\"Combine Content\"].json.aiIntro, $node[\"Combine Content\"].json.aiBenefits, JSON.stringify($node[\"Combine Content\"].json.faqSchema), $node[\"Combine Content\"].json.mainProductImageUrl, `data:image/png;base64,${$node[\"Combine Content\"].json.cityHeroImageBase64}`] }}"
        }
      },
      "name": "Save City Page",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2,
      "position": [1050, 300],
      "credentials": {
        "postgres": {
          "id": "1",
          "name": "GangRun PostgreSQL"
        }
      }
